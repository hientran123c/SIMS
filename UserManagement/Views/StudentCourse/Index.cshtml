@model IEnumerable<UserManagement.Models.StudentCourse>

@{
    ViewData["Title"] = "Student Courses";
}

<h2>Student Courses</h2>

<table>
    <thead>
        <tr>
            <th>Student ID</th>
            <th>Course ID</th>
            <th>Begin Time</th>
            <th>End Time</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td>@course.StudentId</td>
                <td>@course.CourseId</td>
                <td>@course.BeginTime</td>
                <td>@course.EndTime</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@course.Id">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@course.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Create">Add New Course</a>
