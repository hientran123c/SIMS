@model UserManagement.Models.StudentCourse

@{
    ViewData["Title"] = "Create Student Course";
}

<h2>Create Student Course</h2>

<form asp-action="Create" method="post">
    <div>
        <label>Student ID</label>
        <input asp-for="StudentId" />
    </div>
    <div>
        <label>Course ID</label>
        <input asp-for="CourseId" />
    </div>
    <div>
        <label>Begin Time</label>
        <input asp-for="BeginTime" type="datetime-local" />
    </div>
    <div>
        <label>End Time</label>
        <input asp-for="EndTime" type="datetime-local" />
    </div>
    <button type="submit">Create</button>
</form>
